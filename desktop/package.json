{
  "name": "desktop",
  "version": "1.0.0",
  "description": "Contains all our desktop specific code built with electron",
  "main": "index.js",
  "scripts": {
    "start": "electron . --trace-warnings",
    "build:linux": "node build.js linux",
    "build:darwin": "node build.js darwin",
    "build:windows": "node build.js windows",
    "binman": "npx binman .",
    "binman:linux": "npx binman . --platform=linux",
    "binman:windows": "npx binman . --platform=windows",
    "binman:darwin": "npx binman . --platform=darwin"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.2.0",
    "esbuild": "^0.25.10",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "umbr-binman": "^1.0.5"
  }
}
